{
    "name": "hschulz/php-data-structures",
    "description": "Implementation of useful structures.",
    "type": "library",
    "keywords": ["data", "structures"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Hauke Schulz",
            "email": "hauke27@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Hschulz\\DataStructures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hschulz\\DataStructures\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "scripts": {
        "test": "@php vendor/phpunit/phpunit/phpunit --bootstrap=./vendor/autoload.php --whitelist=./src/ --testdox ./Tests/"
    }
}
